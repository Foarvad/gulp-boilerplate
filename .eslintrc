{
  "extends": ["airbnb-base"],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "object-curly-newline": ["error", { "consistent": true }],
    "eol-last": "error"
  }
}
